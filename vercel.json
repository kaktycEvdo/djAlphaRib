{
    "builds": [{
        "src": "index.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "wsgiApplicationName": "alpha",
            "runtime": "python3.11"
        }
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "index.py"
        }
    ]
}
